testouterr_cmd sh {avail -t extra prereq:xtfoo\ @1.0\ foo=bar} OK $tserr
testouterr_cmd sh {avail -t extra prereq:xtfoo\ foo=bar} OK $tserr
testouterr_cmd sh {avail -t extra prereq:xtfoo@0.8:1.2} OK $tserr
