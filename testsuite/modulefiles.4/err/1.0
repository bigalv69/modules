#%Module
if {[info exists env(TESTSUITE_ABORT_ON_ERROR)]} {
    switch -- $env(TESTSUITE_ABORT_ON_ERROR) {
        bad - load_last_bad - load_first_bad - load-any_first_bad - try-load_last_bad - try-load_first_bad - unload_last_bad - unload_first_bad {
            bad
        }
        break - load_first_break - load-any_first_break - try-load_first_break - unload_first_break {
            break
        }
        exit - load_first_exit - load-any_first_exit - try-load_first_exit - unload_first_exit {
            exit
        }
        continue {
            continue
        }
        error - switch_both_error {
            error msg
        }
        conflict {
            conflict foo
        }
        reqmis {
            prereq unk
        }
        depre_load_error {
            prereq --optional foo
            if {[module-info mode load]} {
                error msg
            }
        }
        depre_unload_error {
            prereq --optional foo
            if {[module-info mode unload]} {
                error msg
            }
        }
        depre_load_bad {
            prereq --optional foo
            if {[module-info mode load]} {
                bad
            }
        }
        depre_unload_bad {
            prereq --optional foo
            if {[module-info mode unload]} {
                bad
            }
        }
        depre_load_break {
            prereq --optional foo
            if {[module-info mode load]} {
                break
            }
        }
        depre_unload_break {
            prereq --optional foo
            if {[module-info mode unload]} {
                break
            }
        }
        depre_load_exit {
            prereq --optional foo
            if {[module-info mode load]} {
                exit
            }
        }
        depre_unload_exit {
            prereq --optional foo
            if {[module-info mode unload]} {
                exit
            }
        }
        depre_load_reqmis {
            prereq --optional foo
            if {[module-info mode load]} {
                prereq unk
            }
        }
        depre_load_conflict {
            prereq --optional foo
            if {[module-info mode load]} {
                conflict all
            }
        }
        depre_load_conflict2 {
            prereq --optional foo
            if {[module-info mode load]} {
                conflict foo
            }
        }
        depre_load_forbidden - depre_load_hard_hidden {
            prereq --optional foo
        }
        load_error - reqlo_load_error {
            if {[module-info mode load]} {
                error msg
            }
        }
        depun_error {
            prereq foo
            if {[module-info mode unload]} {
                error msg
            }
        }
        depun_bad {
            prereq foo
            if {[module-info mode unload]} {
                bad
            }
        }
        depun_break - switch_depun_break {
            prereq foo
            if {[module-info mode unload]} {
                break
            }
        }
        depun_exit {
            prereq foo
            if {[module-info mode unload]} {
                exit
            }
        }
        load_bad {
            if {[module-info mode load]} {
                bad
            }
        }
        load_break {
            if {[module-info mode load]} {
                break
            }
        }
        load_exit {
            if {[module-info mode load]} {
                exit
            }
        }
        unload_error - urequn_unload_error - urequn_load_error - conun_load_error - unload_switch_error {
            if {[module-info mode unload]} {
                error msg
            }
        }
        unload_bad - switch_unload_bad {
            if {[module-info mode unload]} {
                bad
            }
        }
        unload_break {
            if {[module-info mode unload]} {
                break
            }
        }
        unload_exit {
            if {[module-info mode unload]} {
                exit
            }
        }
        depre_bad {
            prereq --optional foo
            bad
        }
        depre_break - switch_depre_break {
            prereq --optional foo
            break
        }
        depre_exit {
            prereq --optional foo
            exit
        }
        depre_error {
            prereq --optional foo
            error msg
        }
    }
}
